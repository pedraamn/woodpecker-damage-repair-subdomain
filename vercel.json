{
    "rewrites": [
      {
        "source": "/",
        "has": [
          {
            "type": "host",
            "value": "(?<slug>[a-z0-9-]+)\\.woodpeckerdamagerepaircompany\\.com"
          }
        ],
        "destination": "/:slug/"
      },
      {
        "source": "/(.*)",
        "has": [
          {
            "type": "host",
            "value": "(?<slug>[a-z0-9-]+)\\.woodpeckerdamagerepaircompany\\.com"
          }
        ],
        "destination": "/:slug/:1"
      }
    ]
  }
  